{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "description": "Blank API project to get up and running FAST.",
  "main": "app/server.js",
  "scripts": {
    "build": "babel app --out-dir dist --copy-files",
    "test": "mocha -b --timeout 10000",
    "start": "babel app --out-dir dist --copy-files; node dist/server.js",
    "doc": "apidoc -i dist/ -o apidoc/",
    "debug": "node-debug dist/server.js",
    "startdebug": "node --debug dist/server.js",
    "testdebug": "mocha --debug-brk",
    "coverage": "mocha --require blanket -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/curiosityio/ExpressjsBlanky"
  },
  "config": {
    "blanket": {
      "pattern": "app/controllers/"
    }
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "dependencies": {
    "async": "^1.5.2",
    "bluebird": "^3.4.6",
    "body-parser": "^1.13.2",
    "connect-trim-body": "0.0.3",
    "express": "^4.12.3",
    "express-validator": "^2.19.1",
    "helmet": "^1.3.0",
    "mustache": "^2.2.1",
    "passport": "^0.2.2",
    "passport-http-bearer": "^1.0.1",
    "pg": "^4.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "sqlite3": "^3.1.4",
    "squel": "^4.2.4",
    "uid2": "0.0.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "apidoc": "^0.13.1",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "flow-bin": "^0.33.0",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "node-inspector": "^0.10.2",
    "should": "^8.2.2",
    "supertest": "^1.2.0"
  }
}
